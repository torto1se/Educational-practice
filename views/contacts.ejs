<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
<body>
  <%- include('./partials/nav.ejs') %>
  <div style="margin-top: 200px;  display: flex; flex-direction: column; align-items: center;">
  <h1>Контакты:</h1>
  <ul>
    <% if (contacts.length) { %>
      <% contacts.forEach(({ link, name }) => { %>
        <li>
          <a href="<%= link %>"><%= name %></a>
        </li>
      <% }) %>
    <% } %>
  </ul>
  </div>
</body>
</html>